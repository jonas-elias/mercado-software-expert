{
    "name": "jonaselias/expert-framework",
    "autoload": {
        "psr-4": {
            "Jonaselias\\ExpertFramework\\": "src/"
        }
    },
    "authors": [
        {
            "name": "jonas-elias"
        }
    ],
    "require-dev": {
        "marcocesarato/php-conventional-changelog": "^1.17",
        "phpstan/phpstan": "^1.10"
    },
    "version": "0.2.1",
    "scripts": {
        "generate-changelog": [
            "php vendor/bin/conventional-changelog --commit --config=./src/config/changelog.php"
        ],
        "analyse": [
            "vendor/bin/phpstan analyse ./src"
        ]
    }
}